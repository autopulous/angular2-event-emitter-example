{
  "name": "Angular2 (beta.9) EventEmitter Example",
  "repository": "https://github.com/autopulous/angularjs2-boilerplate.git",
  "scripts": {
    "start gulp watcher": "gulp default",
    "start gulp watcher and lite server": "concurrently \"gulp default\" \"lite-server\"",
    "karma (start continuous tests)": "karma start ./karma.conf.js",
    "karma (run tests once)": "karma start ./karma.conf.js --single-run",
    "server lite (port: 8880 8800)": "lite-server",
    "server http (port: 8888)": "http-server -p 8888 -c-1 --utc",
    "typings": "typings",
    "postinstall": "npm run typings install",
    "npm (list instaled packages)": "npm ls",
    "npm (list outdated packages)": "npm outdated",
    "npm (install packages)": "npm install",
    "npm (simplify duplicate packages)": "npm dedupe",
    "npm (remove unused packages)": "npm prune",
    "npm (cache clear)": "npm cache clean",
    "npm (list cached packages)": "npm cache ls",
    "npm (remove all packages)": "gulp delete-node-modules"
  },
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.3",
